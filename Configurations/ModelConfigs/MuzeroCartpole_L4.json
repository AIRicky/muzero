{
  "name": "singleplayergames_Latent4",
  "algorithm": "MUZERO",
  "architecture": "Gym",

  "args": {
    "num_selfplay_iterations": 100,
    "num_episodes": 100,
    "num_gradient_steps": 200,
    "pitting": true,
    "pitting_trials": 20,
    "pit_acceptance_ratio": 0.0,
    "dirichlet_alpha": 0.3,
    "exploration_fraction": 0.1,
    "max_buffer_size": 200000,
    "num_MCTS_sims": 50,
    "prioritize": false,
    "prioritize_alpha": 0.5,
    "prioritize_beta": 1,
    "latent_decoder": false,
    "K": 5,
    "n_steps": 10,
    "c1": 1.25,
    "c2": 19652,
    "gamma": 0.997,

    "minimum_reward": null,
    "maximum_reward": null,

    "checkpoint": "./out/MuZeroOut/L2/",
    "load_model": false,
    "load_folder_file": ["./out/MuZeroOut/L2/", "latest.pth.tar"],
    "selfplay_buffer_window": 15,

    "temperature_schedule": {
      "method": "stepwise",
      "by_weight_update": true,
      "schedule_points": [[2500, 1], [5000, 0.5], [10000, 0.25]]
    }
  },

  "net_args": {
    "optimizer": {
      "method": "adam",
      "lr_init": 0.02,
      "momentum": 0.9
    },
    "l2": 1e-4,
    "dynamics_penalty": 0,
    "dropout": 0.0,
    "batch_size": 64,
    "cuda": false,
    "latent_depth": 4,
    "num_channels": 256,
    "num_towers": 3,
    "num_dense": 2,
    "size_dense": 128,
    "activation": "tanh",
    "support_size": 20,
    "observation_length": 1
  }
}