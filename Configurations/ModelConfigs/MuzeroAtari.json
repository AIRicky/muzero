{
  "name": "singleplayergames",
  "algorithm": "MUZERO",
  "architecture": "Atari",
  "debug": true,

  "args": {
    "numIters": 100,
    "numEps": 50,
    "numTrainingSteps": 75,
    "pitting": true,
    "pitting_trials": 5,
    "pit_acceptance_ratio": 0.0,
    "dirichlet_alpha": 0.3,
    "exploration_fraction": 0.1,
    "maxlenOfQueue": 200000,
    "numMCTSSims": 50,
    "prioritize": true,
    "prioritize_alpha": 0.5,
    "prioritize_beta": 1,
    "K": 5,
    "n_steps": 10,
    "c1": 1.25,
    "c2": 19652,
    "gamma": 0.997,

    "minimum_reward": null,
    "maximum_reward": null,

    "checkpoint": "./MuZeroOut/",
    "load_model": false,
    "load_folder_file": ["./MuZeroOut/", "latest.pth.tar"],
    "numItersForTrainExamplesHistory": 20,

    "temperature_schedule": {
      "method": "stepwise",
      "by_weight_update": true,
      "schedule_points": [[5000, 1], [7500, 0.5], [10000, 0.25]]
    }
  },

  "net_args": {
    "lr": 0.001,
    "l2": 1e-3,
    "dropout": 0.0,
    "batch_size": 64,
    "cuda": false,
    "num_channels": 256,
    "num_towers": 3,
    "num_dense": 2,
    "size_dense": 64,
    "dense_activation": "tanh",
    "support_size": 20,
    "observation_length": 10
  }
}
