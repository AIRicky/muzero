{
  "name": "singleplayergames",
  "algorithm": "MUZERO",

  "args": {
    "numIters": 10,
    "numEps": 50,
    "numTrainingSteps": 10,
    "pitting": true,
    "pitting_trials": 20,
    "pit_acceptance_ratio": 0.6,
    "tempThreshold": 20,
    "dirichlet_alpha": 0.25,
    "exploration_fraction": 0.25,
    "maxlenOfQueue": 200000,
    "numMCTSSims": 25,
    "prioritize": true,
    "prioritize_alpha": 1,
    "prioritize_beta": 1,
    "K": 5,
    "n_steps": 10,
    "c1": 1.25,
    "c2": 19652,
    "gamma": 0.997,

    "minimum_reward": null,
    "maximum_reward": null,

    "checkpoint": "./MuZeroOut/",
    "load_model": false,
    "load_folder_file": ["./MuZeroOut/", "latest.pth.tar"],
    "numItersForTrainExamplesHistory": 20
  },

  "net_args": {
    "lr": 0.001,
    "l2": 1e-3,
    "dropout": 0.0,
    "batch_size": 64,
    "cuda": false,
    "num_channels": 256,
    "num_towers": 3,
    "len_dense": 2,
    "size_dense": 256,
    "support_size": 0,
    "observation_length": 1
  }
}
