{
  "name": "singleplayergames",
  "algorithm": "MUZERO",

  "args": {
    "numIters": 100,
    "numEps": 100,
    "numTrainingSteps": 10,
    "pitting": true,
    "pitting_trials": 20,
    "pit_acceptance_ratio": 0.0,
    "dirichlet_alpha": 0.25,
    "exploration_fraction": 0.25,
    "maxlenOfQueue": 200000,
    "numMCTSSims": 50,
    "prioritize": true,
    "prioritize_alpha": 0.5,
    "prioritize_beta": 1,
    "K": 10,
    "n_steps": 50,
    "c1": 1.25,
    "c2": 19652,
    "gamma": 0.997,

    "minimum_reward": null,
    "maximum_reward": null,

    "checkpoint": "./MuZeroOut/",
    "load_model": false,
    "load_folder_file": ["./MuZeroOut/", "latest.pth.tar"],
    "numItersForTrainExamplesHistory": 5,

    "temperature_schedule": {
      "method": "stepwise",
      "by_weight_update": true,
      "schedule_points": [[5000, 1], [7500, 0.5], [10000, 0.25]]
    }
  },

  "net_args": {
    "lr": 0.02,
    "l2": 1e-4,
    "dropout": 0.0,
    "batch_size": 128,
    "cuda": false,
    "num_channels": 256,
    "num_towers": 3,
    "num_dense": 1,
    "size_dense": 16,
    "dense_activation": "elu",
    "support_size": 20,
    "observation_length": 1
  }
}
